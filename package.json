{
  "name": "snickerdoodle-protocol",
  "version": "1.0.0",
  "description": "Snickerdoodle Protocol contracts and SDK",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SnickerdoodleLabs/protocol.git"
  },
  "bugs": {
    "url": "https://github.com/SnickerdoodleLabs/protocol/issues"
  },
  "homepage": "https://github.com/SnickerdoodleLabs/protocol#readme",
  "author": "Snickerdoodle Labs",
  "keywords": [
    "Snickerdoodle",
    "Blockchain"
  ],
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "alias": "yarn workspaces foreach -p run alias",
    "bootstrap": "yarn copy-ceramic && yarn copy-ipfs",
    "build": "yarn clean && yarn compile",
    "clean": "npx rimraf ./packages/*/dist ./packages/*/tsconfig.tsbuildinfo",
    "clean-docker": "docker-compose down",
    "clean-node": "npx rimraf node_modules packages/*/node_modules yarn.lock",
    "compile": "npx tsc --build && yarn alias",
    "compile-config": "npx tsc --showConfig",
    "copy-ceramic": "copyfiles -u 1 .ceramic.template/**/* .ceramic",
    "copy-ipfs": "copyfiles -u 1 '.ipfs.template/**/*' .ipfs",
    "docker-pull": "docker-compose pull",
    "docker-push": "yarn workspaces foreach -p run docker-push",
    "dockerize": "yarn build && cd packages/docker && yarn dockerize && cd ../iframe && yarn dockerize && cd ../web-integration-test && yarn dockerize && cd ../extension-onboarding && yarn dockerize && cd ../static-web-integration && yarn dockerize",
    "dockerize-iframe": "yarn build && cd packages/docker && yarn dockerize && cd ../iframe && yarn dockerize",
    "dockerize-spa": "yarn build && cd packages/docker && yarn dockerize && cd ../extension-onboarding && yarn dockerize",
    "dockerize-static-web-integration": "yarn build && cd packages/docker && yarn dockerize && cd ../static-web-integration && yarn dockerize",
    "dockerize-web-integration-test": "yarn build && cd packages/docker && yarn dockerize && cd ../web-integration-test && yarn dockerize",
    "format": "eslint --fix \"packages/**/*.ts\" && yarn format:pkg",
    "format:pkg": "format-package -w",
    "lint": "eslint \"packages/*/{src,test}/**/*.{ts,tsx}\" --quiet",
    "lint:fix": "eslint \"packages/*/{src,test}/**/*.{ts,tsx}\" --quiet --fix",
    "postCompile": "yarn workspaces foreach -p run postCompile",
    "scrap-coin-list": "python3 infra/scrapper/coin_list_gen.py",
    "scrap-price-list": "python3 infra/scrapper/coin_price_gen.py",
    "setup-windows": "yarn install --global --production windows-build-tools",
    "start": "docker-compose up",
    "test": "yarn compile && yarn workspaces foreach -p run test",
    "testScript": "npx run scripts/test.mjs",
    "yeet": "yarn workspaces foreach -p run docker-push",
    "yoink": "docker-compose pull"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.format:pkg": "prettier --write"
  },
  "packageManager": "yarn@3.2.1",
  "resolutions": {
    "@types/react": "^17.0.38",
    "ethereumjs-abi": "^0.6.8",
