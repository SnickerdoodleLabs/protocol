{
  "name": "@snickerdoodlelabs/contracts-sdk",
  "version": "0.0.17",
  "description": "SDK for snickerdoodlelabs contracts wrappers",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SnickerdoodleLabs/protocol.git"
  },
  "bugs": {
    "url": "https://github.com/SnickerdoodleLabs/protocol/issues"
  },
  "homepage": "https://github.com/SnickerdoodleLabs/protocol/tree/master/packages/objects#readme",
  "author": "Muhammed Altinci <muhammed@snickerdoodlelabs.io>",
  "keywords": [
    "Blockchain",
    "Snickerdoodle",
    "Web3"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "!dist/test",
    "!dist/tsconfig.tsbuildinfo",
    "!test",
    "!src",
    "!tsconfig.json"
  ],
  "scripts": {
    "alias": "tsc-alias && tsc-alias -p test/tsconfig.json",
    "alias-with-copyfiles": "yarn copy-files && tsc-alias",
    "build": "yarn clean && yarn compile",
    "clean": "npx rimraf dist tsconfig.tsbuildinfo",
    "compile": "tsc --build && cd ../.. && yarn alias",
    "copy-files": "copyfiles -u 1 src/**/*.d.ts dist/",
    "prepare": "yarn build",
    "prepublish": "yarn build",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --runInBand --coverage --passWithNoTests"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@snickerdoodlelabs/common-utils": "^0.0.7",
    "@snickerdoodlelabs/objects": "workspace:^",
    "ethers": "^5.6.6",
    "inversify": "^5.1.1",
    "neverthrow": "^4.3.1",
    "neverthrow-result-utils": "^1.0.2"
  }
}
