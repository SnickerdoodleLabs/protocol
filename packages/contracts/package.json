{
  "name": "@snickerdoodlelabs/contracts",
  "version": "1.0.0",
  "description": "Smart contracts for the Snickerdoodle Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SnickerdoodleLabs/SDL-Contracts"
  },
  "files": [
    "contracts/",
    "artifacts/"
  ],
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
