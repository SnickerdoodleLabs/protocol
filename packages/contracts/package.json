  },
  "type": "commonjs",
  "dependencies": {
    "@openzeppelin/contracts": "^5.0.2",
    "@openzeppelin/contracts-upgradeable": "^5.0.2",
    "@prb/math": "^4.0.2",
    "@snickerdoodlelabs/erc7529": "workspace:^",
    "dotenv": "^16.0.1",
    "fs": "^0.0.1-security",
    "it-all": "^1.0.6",
    "solidity-coverage": "^0.7.21",
    "solidity-docgen": "^0.6.0-beta.16"
  }
}
