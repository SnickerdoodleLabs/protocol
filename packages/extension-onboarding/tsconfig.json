{
	"extends": "../../tsconfig.build.json",
	"compilerOptions": {
		"outDir": "./dist",
		"rootDir": "./src",
		"baseUrl": "./src",
		"jsx": "react",
		"target": "es5",
		"module": "commonjs",
		"lib": [
			"es5",
			"es6",
			"dom"
		]
	},
	"references": [
		{
			"path": "../objects"
		}
	],
	"include": [
		"./src/**/*.ts",
		"./src/**/*.tsx",
		"index.d.ts"
	],
	"exclude": [
		"node_modules"
	]
}