{
  "name": "@snickerdoodlelabs/static-integration-test",
  "version": "1.0.0",
  "description": "Packages up a javascript file for use on static websites to add Snickerdoodle Data Wallet functionality. Builds to an NGINX docker file to host",
  "main": "dist/index.js",
  "scripts": {
    "alias": "npx tsc-alias",
    "build": "yarn clean && yarn compile && yarn postCompile",
    "clean": "npx rimraf dist tsconfig.tsbuildinfo",
    "compile": "npx tsc --build && cd ../.. && yarn alias",
    "compile-start": "cd ../.. && yarn compile && cd packages/static-web-integration && yarn start",
    "copy-files": "copyfiles -u 1 src/**/*.d.ts dist/",
    "docker-push": "docker push snickerdoodlelabs/static-web-integration:local",
    "dockerize": "docker build -t snickerdoodlelabs/static-web-integration:local -f Dockerfile --progress=plain ../..",
    "postCompile": "npx webpack",
    "prepare": "yarn build",
    "start": "node scripts/webserver.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules npx jest --maxWorkers=50% --coverage --passWithNoTests"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@date-io/date-fns": "1.3.9",
    "@download/blockies": "^1.0.3",
    "@material-ui/core": "4.12.3",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@material-ui/pickers": "3.2.4",
    "@snickerdoodlelabs/objects": "workspace:^",
    "@snickerdoodlelabs/shared-components": "workspace:^",
    "@snickerdoodlelabs/web-integration": "workspace:^",
    "@web3modal/ethereum": "2.7.1",
    "@web3modal/html": "2.7.1",
    "chart.js": "^3.9.1",
    "chroma-js": "^2.4.2",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.1",
    "ethers": "^5.6.6",
    "formik": "2.2.9",
    "formik-material-ui": "3.0.1",
    "gapi-script": "^1.2.0",
    "neverthrow": "^5.1.0",
    "neverthrow-result-utils": "^2.0.2",
    "notistack": "^2.0.5",
    "path-browserify": "^1.0.1",
    "path-to-regexp": "^6.2.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.1",
    "react-google-login": "^5.2.2",
    "react-hotjar": "^5.2.0",
    "react-intersection-observer": "^9.4.3",
    "react-joyride": "^2.5.3",
    "react-lottie": "^1.2.3",
    "react-multi-carousel": "^2.8.2",
    "react-router-dom": "^6.3.0",
    "react-swipeable-views": "^0.14.0",
    "react-swipeable-views-utils": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "url-join-ts": "^1.0.5",
    "url-loader": "^4.1.1",
    "viem": "1.2.5",
    "wagmi": "1.3.9",
    "web3": "^1.6.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
