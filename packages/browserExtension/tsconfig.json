{
  "extends": "../../tsconfig.build.json",
  "compilerOptions": {
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "baseUrl": "./src/",
    "paths": {
      "@interfaces/*": [
        "./extensionCore/interfaces/*"
      ],
      "@implementations/*": [
        "./extensionCore/implementations/*"
      ],
      "@constants/*": [
        "./constants/*"
      ],
      "@shared/*": [
        "./shared/*"
      ],
      "@app/*": [
        "./app/*"
      ],
      "@rpcGateways": [
        "./pages/rpcGateways"
      ],
      "@rpcGateways/*": [
        "./pages/rpcGateways/*"
      ],
      "@utils": [
        "./pages/utils"
      ],
      "@utils/*": [
        "./pages/utils/*"
      ]
    }
  },
  "references": [
    {
      "path": "../core"
    },
    {
      "path": "../objects"
    },
    {
      "path": "../persistence"
    }
  ],
  "include": [
    "src"
  ],
  "exclude": [
    "build",
    "node_modules"
  ]
}