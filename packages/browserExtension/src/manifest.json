{
  "manifest_version": 3,
  "name": "Data-Wallet",
  "description": "Snickerdoodle's Data Wallet",
  "options_page": "options.html",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "oauth2": {
    "client_id": "1055144164647-mfcem81s96av4o1gi6mrmcrqfmbrj5i4.apps.googleusercontent.com",
    "scopes": [
      "profile email"
    ]
  },

  "permissions": ["notifications", "storage", "activeTab", "scripting", "identity", "identity.email", "identity.name", "identity.fullname", "browsingData", "history"],
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*", "<all_urls>"],
    "js": ["contentScript.bundle.js"],
    "css": ["content.styles.css"]
  }],
  "devtools_page": "devtools.html",
  "web_accessible_resources": [{
    "resources": ["content.styles.css", "icon-128.png", "icon-34.png", "shadowScript.js"],
    "matches": ["<all_urls>"]
  }]
}