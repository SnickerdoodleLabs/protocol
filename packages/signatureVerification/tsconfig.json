{
  "extends": "../../tsconfig.build.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "paths": {
      "@signatureVerification/*": [
        "./packages/signatureVerification/src/*"
      ],
      "@signatureVerification-test/*": [
        "./packages/signatureVerification/test/*"
      ],
    },
  },
  "include": [
    "./src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "references": [
    {
      "path": "../objects"
    },
  ]
}